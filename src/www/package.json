{
    "name": "cyfs-git",
    "version": "1.0.4",
    "description": "cyfs git web3 decApp",
    "scripts": {
        "prepare:devtest": "node tools/build_dec_app.js dev-test service",
        "prepare:nightly": "node tools/build_dec_app.js nightly service",
        "www:build:devtest": "cd www && set ENV_TYPE=dev_beta && npx webpack --env production",
        "www:build:nightly": "cd www && cross-env ENV_TYPE=beta npx webpack --env production",
        "www:build:nightly:copy": "npm run www:build:nightly && node tools/copy_js_to_deploy.js",
        "dev": "cd www && cross-env ENV_TYPE=dev npx webpack serve --hot  --mode development --env development",
        "deploy:devtest": "npm run prepare:devtest && npm run www:build:devtest && cd deploy && cyfs deploy",
        "prepare_protoc": "copy /Y 3rd\\protoc\\bin\\protoc.exe node_modules\\.bin\\ && npm i -g ts-protoc-gen",
        "protos_core": "protoc.exe --proto_path=./common/protos --js_out=import_style=commonjs,binary:./common/protos --ts_out=./common/protos ./common/protos/objects.proto",
        "protos": "npm run prepare_protoc && npm run protos_core",
        "lint": "eslint \"**/*.{ts,tsx}\"",
        "lint-fix": "eslint \"**/*.{ts,tsx}\" --fix"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
